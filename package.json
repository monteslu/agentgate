{
  "name": "agentgate",
  "version": "0.8.0",
  "type": "module",
  "description": "API gateway for AI agents with human-in-the-loop write approval",
  "main": "src/index.js",
  "bin": {
    "agentgate": "src/cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "src/",
    "public/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monteslu/agentgate.git"
  },
  "keywords": [
    "ai",
    "agent",
    "api",
    "gateway",
    "proxy",
    "human-in-the-loop",
    "llm",
    "claude",
    "gpt"
  ],
  "author": "Luis Montes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monteslu/agentgate/issues"
  },
  "homepage": "https://github.com/monteslu/agentgate#readme",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "keys": "node src/cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "tui": "node src/tui/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.0.0",
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "hsync": "^0.33.0",
    "nanoid": "^5.0.0",
    "socket.io": "^4.8.3",
    "stemmer": "^2.0.1",
    "terminal-kit": "^3.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
